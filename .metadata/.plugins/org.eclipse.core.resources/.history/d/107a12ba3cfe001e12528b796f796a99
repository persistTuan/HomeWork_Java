package managermentEmployee;

import java.util.ArrayList;
import java.util.List;

public class Main {
	public boolean removeEmployee(List<Employee> listEmployee, String code) {
		for (Employee ls : listEmployee) {
			if(ls.getCode() == code)
			{
				return listEmployee.remove(ls);
			}
		}
		return false;
	}
